<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Home">

    <TextView
            android:text="Bienvenido..."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView" android:textSize="40sp"
            android:layout_marginTop="184dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/textView3"
            app:layout_constraintVertical_chainStyle="spread_inside" app:layout_constraintVertical_bias="0.35000002"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="49dp"
            android:id="@+id/userName" android:textSize="30sp"
            app:layout_constraintTop_toBottomOf="@+id/textView3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="144dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="144dp"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="40dp"/>
    <TextView
            android:text="Welcome..."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" android:textSize="30sp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/userName"
            android:layout_marginTop="45dp" app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="130dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="131dp"
            app:layout_constraintHorizontal_bias="0.0"/>
</androidx.constraintlayout.widget.ConstraintLayout>